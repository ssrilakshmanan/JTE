pipeline {
	agent any
	tools {
		maven "Maven 3.0"
	}
	stages {
        stage('Build') { 
            steps { 
                echo 'Build'
				mvn clean
            }
        }
        stage('Test'){
            steps {
                echo 'Test' 
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy' 
            }
        }
    }
}